<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>netstandard2.1;net461</TargetFrameworks>
	  <Authors>xhq</Authors>
	  <Company>xhq</Company>
	  <Description>基于Mqttnet的mqttclient封装
Mqttnet连接：https://www.nuget.org/packages/MQTTnet/
Mqttnet GitHub:https://github.com/chkr1011/MQTTnet</Description>
	  <PackageLicenseExpression></PackageLicenseExpression>
	  <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <PackageLicenseFile>MIT.txt</PackageLicenseFile>
	  <Copyright />
	  <PackageProjectUrl>https://github.com/GlibWild/ExMqttClient</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/GlibWild/ExMqttClient</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MQTTnet" Version="3.0.15" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\License\MIT.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
